#
# nek parameter file
#
[GENERAL] 

#startFrom = start0.f04000 time=0.
stopAt = endTime
endTime = 2000

dt = 1.
timeStepper = bdf3
variableDt = yes
targetCFL = 0.5

writeControl = runTime
writeInterval = 0.5

filtering = none

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
preconditioner = semg_XXT
residualTol = 1e-4
residualProj = yes

[VELOCITY]
residualTol = 1e-4
density = 1
viscosity = -2000

