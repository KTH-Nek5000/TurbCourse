#
# nek parameter file
#
[GENERAL] 

startFrom = start0.f04000 time=0.
stopAt = endTime
endTime = 1000

dt = 0.01
timeStepper = bdf3
variableDt = yes
targetCFL = 0.5

writeControl = runTime
writeInterval = 0.5

filtering = none
#filtering = hpfrt
#filterWeight = 0.0000001
#filterCutoffRatio = 0.9 

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
preconditioner = semg_XXT
residualTol = 1e-4
residualProj = yes

[VELOCITY]
residualTol = 1e-6
density = 1
viscosity = -2000

